<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".UtstyrSlidePageFragment">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="22sp"
        android:layout_margin="10dp"
        android:text="@string/utstyr" />
    <TextView
        android:id="@+id/tvProdusent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="22sp"
        android:layout_margin="10dp"
        android:background="#2196F3"
        android:text="@string/str_ff"/>

    <ImageView
        android:id="@+id/ivBilde"
        android:layout_width="wrap_content"
        android:layout_height="220dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="10dp"
        android:contentDescription="@string/ddd"
        android:minWidth="200dp"
        android:scaleType="fitXY" />

    <EditText
        android:id="@+id/etDittNavn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:hint="@string/navn"
        android:inputType="text"
        android:textSize="22sp"
        android:autofillHints="" />

    <TextView
        android:id="@+id/tvURL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/textview" />
</LinearLayout>