<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginStart="10dp"
    android:layout_marginTop="80dp"
    android:layout_marginEnd="10dp"
    android:layout_marginBottom="10dp"
    android:padding="5dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">


    <TextView
        android:id="@+id/tvA"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"
        android:background="@drawable/tv_buttons_background_portrait_grey"

        android:freezesText="true"
        android:paddingBottom="50dp"
        android:textAlignment="center"
        android:textAppearance="@style/textStyleButtons"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvB"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"

        android:layout_marginStart="4dp"
        android:background="@drawable/tv_buttons_background_portrait_grey"
        android:freezesText="true"
        android:paddingBottom="30dp"
        android:textAlignment="center"
        android:textAppearance="@style/textStyleButtons"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toEndOf="@+id/tvA"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvC"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"
        android:layout_marginStart="4dp"

        android:background="@drawable/tv_buttons_background_portrait_grey"
        android:freezesText="true"
        android:textAlignment="center"
        android:textAppearance="@style/textStyleButtons"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toEndOf="@+id/tvB"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvD"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"
        android:layout_marginStart="8dp"

        android:layout_marginTop="8dp"
        android:background="@drawable/tv_buttons_background_portrait_grey"
        android:freezesText="true"
        android:textAlignment="center"
        android:textAppearance="@style/textStyleButtons"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvA" />

    <TextView
        android:id="@+id/tvE"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/tv_buttons_background_portrait_grey"

        android:freezesText="true"
        android:textAlignment="center"
        android:textAppearance="@style/textStyleButtons"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toEndOf="@+id/tvD"
        app:layout_constraintTop_toBottomOf="@+id/tvB" />

    <TextView
        android:id="@+id/tvF"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/tv_buttons_background_portrait_grey"

        android:freezesText="true"
        android:textAlignment="center"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toEndOf="@+id/tvE"
        app:layout_constraintTop_toBottomOf="@+id/tvC" />

    <TextView
        android:id="@+id/tvG"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/tv_buttons_background_portrait_grey"

        android:freezesText="true"
        android:textAlignment="center"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvD" />

    <TextView
        android:id="@+id/tvH"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/tv_buttons_background_portrait_grey"

        android:freezesText="true"
        android:textAlignment="center"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toEndOf="@+id/tvG"
        app:layout_constraintTop_toBottomOf="@+id/tvE" />

    <TextView
        android:id="@+id/tvK"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_margin="4dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/tv_buttons_background_portrait_grey"

        android:freezesText="true"
        android:textAlignment="center"
        android:theme="@style/textStyleButtons"
        app:layout_constraintStart_toEndOf="@+id/tvH"
        app:layout_constraintTop_toBottomOf="@+id/tvF" />

    <TextView
        android:id="@+id/tvSpillerX"
        android:layout_width="140dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/tv_spiller_background_grey"
        android:maxLines="2"
        android:paddingStart="15dp"
        android:paddingTop="10dp"
        android:paddingEnd="15dp"
        android:text="@string/StringSpiller_x"

        android:textAlignment="center"
        android:textAppearance="@style/textStylePlayers"
        android:textColor="#56219F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvSpillerO"
        app:layout_constraintHorizontal_bias="0.828"
        app:layout_constraintStart_toEndOf="@+id/tvK"
        app:layout_constraintTop_toBottomOf="@+id/tvElapsetTid"
        app:layout_constraintVertical_bias="0.027" />

    <TextView
        android:id="@+id/tvSpillerO"
        android:layout_width="140dp"
        android:layout_height="80dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/tv_spiller_background_grey"
        android:maxLines="2"
        android:paddingStart="15dp"
        android:paddingTop="10dp"
        android:paddingEnd="15dp"
        android:text="@string/StringSpiller_o"
        android:textAlignment="center"

        android:textAppearance="@style/textStylePlayers"
        android:textColor="#CA2A72"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvElapsetTid"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tvElapsetTid"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="31dp"
        android:layout_marginBottom="29dp"
        android:text="@string/Stringl00_00"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/tvF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toEndOf="@+id/tvF"
        app:layout_constraintTop_toTopOf="@+id/tvF"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tvResultat1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="24dp"
        android:text="@string/StringResultat"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/tvC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.335"
        app:layout_constraintStart_toEndOf="@+id/tvC"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.459" />

    <TextView
        android:id="@+id/tvResultat"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:hint="@string/StringForelopig_ingen_seier"
        android:maxLines="2"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tvElapsetTid"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.094"
        app:layout_constraintStart_toEndOf="@+id/tvC"
        app:layout_constraintTop_toBottomOf="@+id/tvResultat1"
        app:layout_constraintVertical_bias="0.538" />

</androidx.constraintlayout.widget.ConstraintLayout>